{
    "containerDefinitions": [
        {
            "name": "httpd",
            "image": "PLACEHOLDER",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "environment": [
                {"name": "devx-test", "value": "woo-hoo!" },
                {"name": "foo", "value": "bar"}
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "httpd"
}