{
    "containerDefinitions": [
        {
            "name": "web",
            "image": "PLACEHOLDER",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "environment": [
                {"name": "WEB", "value": "v1" }
            ]
        },
        {
            "name": "ecssample",
            "image": "amazon/amazon-ecs-sample",
            "environment": [
                {"name": "ECS", "value": "test" }
            ],
            "dockerLabels": {
                "SOME_TEXT": "FAKE LABEL"
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "web"
}